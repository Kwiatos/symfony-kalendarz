<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap 101 Template</title>

    <!-- Bootstrap -->

{% block block_css %}

	{% stylesheets
		'bundles/smsapicommonlayout/lib/bootstrap-3.2.0-dist/css/bootstrap.min.css'
	filter='cssrewrite' %}
	    <link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}

{% endblock %}


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    {% javascripts
	    'https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js'
	    'https://oss.maxcdn.com/respond/1.4.2/respond.min.js'
	    %}
	    <script src="{{ asset_url }}"></script>
	{% endjavascripts %}
    <![endif]-->

{% block block_javascript_top %}
	{% javascripts
	    'https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js'
	    %}
	    <script src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}
  </head>
  <body>

{% block content %}
{% endblock %}


{% block block_javascript %}
	{% javascripts
	    '@SMSApiCommonLayoutBundle/Resources/public/lib/bootstrap-3.2.0-dist/js/bootstrap.min.js'
	    '@SMSApiCommonLayoutBundle/Resources/public/lib/jquery-ui.custom.min.js'
	    '@SMSApiCommonLayoutBundle/Resources/public/lib/moment.min.js'
	    %}
	    <script src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}
  </body>
</html>

